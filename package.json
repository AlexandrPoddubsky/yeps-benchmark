{
  "name": "yeps-benchmark",
  "version": "0.0.1",
  "description": "Benchmark for yeps",
  "main": "index.js",
  "config": {
    "port": "3000",
    "count": "10",
    "concurrency": "100",
    "requests": "50000"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint *.js",
    "test": "./node_modules/.bin/npm-run-all test:*",
    "test:http": "sh bin/test http $npm_package_config_count $npm_package_config_port $npm_package_config_concurrency $npm_package_config_requests > http.txt",
    "test:express": "sh bin/test express $npm_package_config_count $npm_package_config_port $npm_package_config_concurrency $npm_package_config_requests > express.txt",
    "test:koa": "sh bin/test koa2 $npm_package_config_count $npm_package_config_port $npm_package_config_concurrency $npm_package_config_requests > koa2.txt",
    "test:yeps": "sh bin/test yeps $npm_package_config_count $npm_package_config_port $npm_package_config_concurrency $npm_package_config_requests > yeps.txt",
    "precommit": "npm run lint",
    "prepush": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evheniy/yeps-benchmark.git"
  },
  "keywords": [
    "yeps",
    "promise",
    "http",
    "server",
    "rest",
    "fast",
    "async",
    "await",
    "https",
    "ssl",
    "easy"
  ],
  "author": "Evheniy Bystrov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evheniy/yeps-benchmark/issues"
  },
  "homepage": "https://github.com/evheniy/yeps-benchmark#readme",
  "dependencies": {
    "express": "^4.14.1",
    "koa": "^2.0.0-alpha.8",
    "nodeload": "^0.4.0",
    "promise-pause-timeout": "0.0.1",
    "yeps": "^0.0.10"
  },
  "devDependencies": {
    "eslint": "^3.15.0",
    "eslint-config-eslint": "^3.0.0",
    "husky": "^0.13.1",
    "npm-run-all": "^4.0.1",
    "nsp": "^2.6.2",
    "rimraf": "^2.5.4"
  }
}
